@using System.Configuration
@using Microsoft.AspNet.Identity
@using VXI_GAMS_US.Controllers
@{
    ViewBag.Title = "HOME";
}
<section class="content" ng-controller="homeCtrl as vm">
    <div class="container-fluid">
        @*<span class="label label-default normal">For dropdown issue, press <strong>Ctrl</strong>+<strong>Shift</strong>+<strong>R</strong> on your keyboard</span>
            <span class="label label-warning normal">For access issue, click "<strong>WAH Remote Support</strong>" logo above</span>*@
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="card">
                    <div class="body p-t-5">
                        <!-- Nav tabs -->
                        <ul class="nav nav-tabs" role="tablist">
                            @if (Session["Account"] is SessionUser ee &&
                                 (ee.UserRole.Any(x => x.ItemName.ToUpper() == "IT")
                                  || ee.UserRole.Any(x => x.ItemName.ToUpper() == "REAF")
                                  || ee.UserRole.Any(x => x.ItemName.ToUpper() == "ADMINISTRATOR")))
                            {

                                <li role="presentation" class="active"><a href="#farm-tab" data-toggle="tab" aria-expanded="true">FARM</a></li>
                                <li role="presentation"><a href="#home" data-toggle="tab" aria-expanded="true">ASSET RECORDS</a></li>
                                <li role="presentation"><a href="#transaction-tab" data-toggle="tab" aria-expanded="true">TRANSACTION</a></li>
                            }
                            @if (Session["Account"] is SessionUser eee && eee.UserRole.Any(x => x.ItemName.ToUpper().Contains("APPROVER")))
                            {
                                <li role="presentation"><a href="#approver-tab" data-toggle="tab" aria-expanded="true">APPROVER</a></li>
                            }
                        </ul>

                        @{
                            var active1 = string.Empty;
                            var active2 = string.Empty;
                            if (Session["Account"] is SessionUser e && e.UserRole.Any(x => x.ItemName.ToUpper().Contains("APPROVER")))
                            {
                                var test = new string[] { "IT", "REAF", "ADMINISTRATOR" };

                                if (!e.UserRole.Any(x => test.Any(c => c == x.ItemName)))
                                {
                                    active1 = string.Empty;
                                    active2 = "active in";
                                }
                                else
                                {
                                    active1 = "active in";
                                    active2 = string.Empty;
                                }
                            }
                            else
                            {
                                active1 = "active in";
                                active2 = string.Empty;
                            }
                        }
                        <!-- Tab panes -->
                        <div class="tab-content">
                            <div role="tabpanel" class="tab-pane fade" id="home">
                                <!-- Select -->
                                <div class="row clearfix">

                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 m-b-0">
                                        <div class="col-sm-12">
                                            <div class="row clearfix">
                                                <input type="checkbox" id="basic_checkbox_3" ng-model="vm.useBulkUpload">
                                                <label for="basic_checkbox_3">USE BULK UPLOADING</label>
                                            </div>
                                        </div>
                                    </div>
                                    <div ng-show="vm.useBulkUpload" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 m-b-0">
                                        <div class="col-sm-12">
                                            <div class="row clearfix">
                                                <div class="col-sm-12">
                                                    <div class="form-group">
                                                        <input type="file" id="asset-bulk-container" ng-excel-sheet-import="newAssetBulkItems" accept="application/vnd.ms-excel, application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" class="ng-isolate-scope" style="width: 100%;">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div ng-show="vm.useBulkUpload" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 m-b-0">
                                        <div class="col-sm-12">
                                            <div class="row clearfix">
                                                <a href="@Url.Action("Template", "Home")" id="download-template-botton" class="waves-effect waves-green btn orange" style="margin-left: 16px;">DOWNLOAD TEMPLATE</a>
                                                <button class="btn btn-primary waves-effect btn-lg" type="submit" style="margin-left: 16px;" ng-click="vm.uploadAssetBulkData()" ng-bind="vm.isSaveOngoing === false ? 'UPLOAD ('+vm.AssetBulkItems.Asset.length+') ITEMS':'SAVING...'" ng-disabled="vm.AssetBulkItems.Asset.length < 1 || vm.isSaveOngoing"></button>
                                            </div>
                                        </div>
                                    </div>
                                    <div ng-hide="vm.useBulkUpload" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 m-b-0">
                                        <div class="row clearfix">
                                            <button class="btn bg-deep-orange waves-effect btn-lg" type="submit" style="margin-left: 16px;" ng-click="vm.openAssetModal()">NEW ASSET</button>
                                            <button class="btn bg-cyan waves-effect btn-lg float-right m-r-15" type="button" ng-click="vm.openQrCodeModal()">SCAN QR CODE</button>
                                            <button class="btn bg-teal waves-effect btn-lg float-right m-r-15" type="button" ng-click="vm.openAssetWindow()">SEARCH</button>
                                        </div>
                                    </div>
                                    <div ng-hide="vm.useBulkUpload" class="col-lg-4 col-md-4 col-sm-12 col-xs-12 m-b-0">
                                        <div class="row clearfix">
                                            <div class="col-sm-12 m-t-10 img-container">
                                                <b>IMAGE</b>
                                                <span class="qrcode-container">
                                                    <img ng-if="vm.Asset.ImageFile !== null && vm.Asset.ImageFile !== undefined && vm.Asset.ImageFile.length > 0" ng-src="{{vm.Asset.ImageFile}}" loading="lazy" class="img" alt="Asset Image" title="Asset Image" />
                                                    <img ng-if="vm.Asset.ImageFile === null || vm.Asset.ImageFile === undefined || vm.Asset.ImageFile.length < 1" src="~/public/image/img_placeholder.png" loading="lazy" class="img" alt="Asset Image" title="Asset Image" />
                                                </span>
                                            </div>
                                        </div>
                                        <div class="row clearfix">
                                            <div class="col-sm-12 align-center">
                                                <div class="form-group" style="margin-bottom: 8px;">
                                                    <input type="file" ng-model="vm.UploadingFile.Image" class="no-display" id="new-asset-img" accept="image/*" file-upload-on-change="setImage" />
                                                    <button type="button" class="btn btn-block waves-effect shadowless upload-btn">UPLOAD PHOTO</button>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row clearfix">
                                            <div class="col-sm-12 m-t-10 img-container">
                                                <b>QR CODE</b>
                                                <span class="qrcode-container">
                                                    <img ng-if="vm.Asset.QRCode !== null && vm.Asset.QRCode !== undefined && vm.Asset.QRCode.length > 0" ng-src="{{vm.Asset.QRCode}}" loading="lazy" class="img" alt="QR CODE" title="QR CODE" />
                                                    <img ng-if="vm.Asset.QRCode === null || vm.Asset.QRCode === undefined || vm.Asset.QRCode.length < 1" src="~/public/image/img_placeholder.png" loading="lazy" class="img" alt="QR CODE" title="QR CODE" />
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                    <div ng-hide="vm.useBulkUpload" class="col-lg-4 col-md-4 col-sm-12 col-xs-12 m-b-0">

                                        <form>
                                            <div class="row clearfix">
                                                <div class="col-sm-12 m-t-10">
                                                    <b>DESCRIPTION @*<span class="required-field">*</span>*@</b>
                                                    <div class="form-group">
                                                        <div class="form-line">
                                                            <input type="text" readonly="readonly" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.Description">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row clearfix">
                                                <div class="col-sm-12 m-t-10">
                                                    <b>CATEGORY</b>
                                                    <div class="form-group">
                                                        <select class="form-control show-tick" id="select-openrecords-category" ng-options="x.ItemName for x in vm.Categories track by x.Id" ng-model="vm.Asset.Category" title="Select an Item..." data-live-search="true" my-select>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row clearfix">
                                                <div class="col-sm-12 m-t-10">
                                                    <b>SUBCATEGORY @*<span class="required-field">*</span>*@</b>
                                                    <div class="form-group">
                                                        <select class="form-control show-tick" id="select-openrecords-subcategory" ng-options="x.ItemName for x in vm.SubCategories track by x.Id" ng-model="vm.Asset.SubCategory" title="Select an Item..." data-live-search="true" my-select>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row clearfix">
                                                <div class="col-sm-12 m-t-10">
                                                    <b>SITE @*<span class="required-field">*</span>*@</b>
                                                    <div class="form-group">
                                                        <select disabled="disabled" class="form-control show-tick" id="select-openrecords-site" ng-options="x.ItemName for x in vm.Sites track by x.ItemId" ng-model="vm.Asset.Site" title="Select an Item..." data-live-search="true" my-select>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row clearfix">
                                                <div class="col-sm-12 m-t-10">
                                                    <b class="to-upper">WORK TYPE</b>
                                                    <div class="form-group">
                                                        <select class="form-control show-tick" id="asset-work-type" ng-model="vm.Asset.WorkType" title="Select an Item..." data-live-search="true" my-select>
                                                            <option ng-repeat="x in vm.WorkTypes" value="{{x}}">{{x}}</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row clearfix" ng-if="vm.Asset.WorkType === 'WAS'">
                                                <div class="col-sm-12 m-t-10">
                                                    <b>FLOOR</b>
                                                    <div class="form-group">
                                                        <div class="form-line">
                                                            <input type="text" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.FLoor">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row clearfix" ng-if="vm.Asset.WorkType === 'WAS'">
                                                <div class="col-sm-12 m-t-10">
                                                    <b>AREA/WORKSTATION</b>
                                                    <div class="form-group">
                                                        <div class="form-line">
                                                            <input type="text" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.AreaWorkStation">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row clearfix" ng-if="vm.Asset.WorkType === 'WAH'">
                                                <div class="col-sm-12 m-t-10">
                                                    <b>HOME ADDRESS</b>
                                                    <div class="form-group">
                                                        <div class="form-line">
                                                            <input type="text" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.HomeAddress">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row clearfix">
                                                <div class="col-sm-12 m-t-10">
                                                    <b>EMPLOYEE</b>
                                                    <div class="form-group">
                                                        <div class="form-line">
                                                            <input readonly="readonly" type="text" class="form-control" placeholder="Enter here..." maxlength="200" value="{{vm.Asset.Employee.HRID}} - {{vm.Asset.Employee.FirstName}} {{vm.Asset.Employee.LastName}}">
                                                        </div>
                                                        <button ng-disabled="vm.Asset.Description === null || vm.Asset.Description === undefined || vm.Asset.Description.length < 1" class="btn bg-teal waves-effect btn-lg" type="button" ng-click="vm.openEmployeeWindow()">SEARCH EMPLOYEE</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                    <div ng-hide="vm.useBulkUpload" class="col-lg-4 col-md-4 col-sm-12 col-xs-12 m-b-0">

                                        <form>
                                            <div class="row clearfix">
                                                <div class="col-sm-12 m-t-10">
                                                    <b class="to-upper">CONTROL NO</b>
                                                    <div class="form-group">
                                                        <div class="form-line">
                                                            <input type="text" readonly="readonly" class="form-control" placeholder="(Autogenerated)" maxlength="200" ng-model="vm.Asset.ControlNo">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row clearfix">
                                                <div class="col-sm-12 m-t-10">
                                                    <b>VENDOR @*<span class="required-field">*</span>*@</b>
                                                    <div class="form-group">
                                                        <select disabled="disabled" class="form-control show-tick" id="select-openrecords-vendor" ng-options="x.ItemName for x in vm.Vendors track by x.Id" ng-model="vm.Asset.Vendor" title="Select an Item..." data-live-search="true" my-select>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row clearfix">
                                                <div class="col-sm-12 m-t-10">
                                                    <b>SERIAL NO @*<span class="required-field">*</span>*@</b>
                                                    <div class="form-group">
                                                        <div class="form-line">
                                                            <input type="text" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.SerialNo">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row clearfix">
                                                <div class="col-sm-12 m-t-10">
                                                    <b>SALES INVOICE</b>
                                                    <div class="form-group">
                                                        <div class="form-line">
                                                            <input type="text" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.SalesInvoice">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row clearfix">
                                                <div class="col-sm-12 m-t-10">
                                                    <b>GR NO</b>
                                                    <div class="form-group">
                                                        <div class="form-line">
                                                            <input type="text" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.GrNo">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row clearfix">
                                                <div class="col-sm-12 m-t-10">
                                                    <b>LOB OWNER @*<span class="required-field">*</span>*@</b>
                                                    <div class="form-group">
                                                        <select class="form-control show-tick" id="lobOwner" ng-options="x.ItemName for x in vm.Lobs track by x.Id" ng-model="vm.Asset.LobOwner" title="Select an Item..." data-live-search="true" my-select>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row clearfix">
                                                <div class="col-sm-12 m-t-10">
                                                    <b>BRAND @*<span class="required-field">*</span>*@</b>
                                                    <div class="form-group">
                                                        <select class="form-control show-tick" id="select-openrecords-brand" ng-options="x.ItemName for x in vm.Brands track by x.Id" ng-model="vm.Asset.Brand" title="Select an Item..." data-live-search="true" my-select>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row clearfix">
                                                <div class="col-sm-12 m-t-10">
                                                    <b>MODEL @*<span class="required-field">*</span>*@</b>
                                                    <div class="form-group">
                                                        <div class="form-line">
                                                            <input type="text" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.Model">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                    <div ng-hide="vm.useBulkUpload" class="col-sm-12">
                                        <div class="row clearfix">
                                            <button class="btn btn-primary waves-effect btn-lg" type="submit" style="float: right; margin-right: 15px;" ng-click="vm.saveNewAsset()" ng-bind="vm.isSaveOngoing === false ? 'SAVE CHANGES':'SAVING...'" ng-disabled="vm.Asset.Description === null || vm.Asset.Description === undefined || vm.Asset.Description.length < 1 || vm.isSaveOngoing"></button>
                                        </div>
                                    </div>
                                    <div ng-hide="vm.useBulkUpload" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 m-b-0">
                                        <ul class="nav nav-tabs" role="tablist">
                                            <li role="presentation" class="active"><a href="#general-tab" data-toggle="tab" aria-expanded="true">GENERAL</a></li>
                                            <li role="presentation"><a href="#financial-tab" data-toggle="tab" aria-expanded="true">FINANCIAL</a></li>
                                            @*<li role="presentation"><a href="#peza-tab" data-toggle="tab" aria-expanded="true">PEZA</a></li>*@
                                            <li role="presentation"><a href="#maintenance-tab" data-toggle="tab" aria-expanded="true">MAINTENANCE AGREEMENT</a></li>
                                            <li role="presentation"><a href="#contract-tab" data-toggle="tab" aria-expanded="true">CONTRACT</a></li>
                                            <li role="presentation"><a href="#attachments-tab" data-toggle="tab" aria-expanded="true">ATTACHMENTS</a></li>
                                            <li role="presentation"><a href="#history-tab" data-toggle="tab" aria-expanded="true">HISTORY</a></li>
                                        </ul>
                                        <div class="tab-content">
                                            <div role="tabpanel" class="tab-pane fade active in" id="general-tab">
                                                <div class="row clearfix">
                                                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 m-b-0">
                                                        <div class="row clearfix">
                                                            <div class="col-sm-12 m-t-10">
                                                                <b>STATUS</b>
                                                                <div class="form-group">
                                                                    <select class="form-control show-tick" id="select-openrecords-status" ng-options="x.ItemName for x in vm.AssetStatus track by x.Id" ng-model="vm.Asset.AssetStatus" title="Select an Item..." data-live-search="true" my-select>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row clearfix">
                                                            <div class="col-sm-12 m-t-10">
                                                                <b>TAG DATE</b>
                                                                <div class="form-group">
                                                                    <div class="form-line">
                                                                        <input type="date" disabled="disabled" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.TagDate">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row clearfix">
                                                            <div class="col-sm-12 m-t-10">
                                                                <b>DELIVERY DATE</b>
                                                                <div class="form-group">
                                                                    <div class="form-line">
                                                                        <input type="date" disabled="disabled" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.DeliveryDate">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row clearfix">
                                                            <div class="col-sm-12 m-t-10">
                                                                <b>GROUP</b>
                                                                <div class="form-group">
                                                                    <select disabled class="form-control show-tick" id="group2" ng-options="x.ItemName for x in vm.Groups track by x.Id" ng-model="vm.Asset.Group" title="No Assigned Group" data-live-search="true" my-select>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 m-b-0">
                                                        <div class="row clearfix">
                                                            <div class="col-sm-12 m-t-10">
                                                                <b>WARRANTY START DATE</b>
                                                                <div class="form-group">
                                                                    <div class="form-line">
                                                                        <input type="date" disabled="disabled" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.WarrantyStartDate">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row clearfix">
                                                            <div class="col-sm-12 m-t-10">
                                                                <b>WARRANTY END DATE</b>
                                                                <div class="form-group">
                                                                    <div class="form-line">
                                                                        <input type="date" disabled="disabled" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.WarrantyEndDate">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row clearfix">
                                                            <div class="col-sm-12 m-t-10">
                                                                <b>NOTES</b>
                                                                <div class="form-group">
                                                                    <div class="form-line">
                                                                        <textarea rows="4" class="form-control no-resize" placeholder="Enter here..." ng-model="vm.Asset.Remarks" style="height: 118px;"></textarea>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div role="tabpanel" class="tab-pane fade" id="financial-tab">
                                                <div class="row clearfix">
                                                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 m-b-0">
                                                        <div class="row clearfix">
                                                            <div class="col-sm-12 m-t-10">
                                                                <b>DELIVERY NO.</b>
                                                                <div class="form-group">
                                                                    <div class="form-line">
                                                                        <input disabled="disabled" type="text" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.DeliveryNo">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row clearfix">
                                                            <div class="col-sm-12 m-t-10">
                                                                <b>PURCHASE DATE</b>
                                                                <div class="form-group">
                                                                    <div class="form-line">
                                                                        <input disabled="disabled" type="date" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.PurchaseDate">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row clearfix">
                                                            <div class="col-sm-12 m-t-10">
                                                                <b>PURCHASE ORDER NO.</b>
                                                                <div class="form-group">
                                                                    <div class="form-line">
                                                                        <input disabled="disabled" type="text" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.PuchaseOrderNo">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row clearfix">
                                                            <div class="col-sm-12 m-t-10">
                                                                <b>INVOICE NO.</b>
                                                                <div class="form-group">
                                                                    <div class="form-line">
                                                                        <input disabled="disabled" type="text" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.InvoiceNo">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row clearfix">
                                                            <div class="col-sm-12 m-t-10">
                                                                <b>Service Date</b>
                                                                <div class="form-group">
                                                                    <div class="form-line">
                                                                        <input type="date" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.ServiceDate">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row clearfix">
                                                            <div class="col-sm-12 m-t-10">
                                                                <b>PURCHASE PRICE</b>
                                                                <div class="form-group">
                                                                    <div class="form-line">
                                                                        <input disabled="disabled" type="text" class="form-control" placeholder="Enter here..." maxlength="200" ng-model="vm.Asset.PurchasePrice">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 m-b-0">
                                                        <div class="row clearfix">
                                                            <div class="col-sm-12 m-t-10">
                                                                <b>FINANCIAL TREATMENT</b>
                                                                <div class="form-group">
